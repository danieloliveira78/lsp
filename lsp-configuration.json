{
  "comments": {
      "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["inicio", "fim"]
  ],
  "autoClosingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
  { "open": "'", "close": "'", "notIn": ["string", "comment"] },
  { "open": "\"", "close": "\"", "notIn": ["string"] },
  { "open": "`", "close": "`", "notIn": ["string", "comment"] },
  { "open": "@", "close": "@", "notIn": ["string", "comment"] },
  { "open": "/**", "close": " */", "notIn": ["string"] },
  { "open": "inicio", "close": " fim;", "notIn": ["string", "comment"] }
  ],
  "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
  ],
  "indentationRules": {
      "increaseIndentPattern": "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*|(inicio|Inicio))$",
      "decreaseIndentPattern": "^((?!.*?\\/\\*).*\\*/)?\\s*[\\)\\}\\]].*$"
  },
  "folding": {
      "markers": {
        "start": "^\\s*@--\\s*#?(bloco|Bloco)\\b(\\s\\'.*\\')?\\s*--@",
        "end": "^\\s*@--\\s*#?(fimbloco|FimBloco|Fimbloco)\\b(\\s\\'.*\\')?\\s*--@"
      }
    }
}
